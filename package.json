{
  "name": "coronate",
  "version": "1.1.1",
  "description": "Swiss chess tournament manager",
  "author": {
    "name": "John Jackson",
    "email": "johnbicyclejackson@icloud.com",
    "url": "https://johnridesa.bike/"
  },
  "license": "AGPL-3.0-or-later",
  "private": true,
  "homepage": ".",
  "repository": {
    "type": "git",
