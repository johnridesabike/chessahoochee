  },
  "scripts": {
    "build": "npm run clean:res && npm run build:res && node scripts/build.js",
    "deploy:github-pages": "npm run build && gh-pages -d build",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --watchAll",
    "test:ci": "node scripts/test.js --watchAll=false",
    "test:coverage": "npm run test -- --coverage",
    "build:res": "bsb -make-world",
    "start:res": "bsb -make-world -w",
    "clean:res": "bsb -clean-world"
  },
